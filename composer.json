{
    "name": "serafim/castxml",
    "description": "CastXml wrapper for PHP",
    "keywords": ["xml", "ast", "parser", "ansi c", "c", "castxml", "ffi"],
    "license": "MIT",
    "require": {
        "php": ">=7.4",
        "symfony/process": ">=4.4",
        "symfony/polyfill-php80": "^1.22"
    },
    "autoload": {
        "psr-4": {
            "Serafim\\CastXml\\": "src"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "vimeo/psalm": "^4.7",
        "symfony/dom-crawler": ">=4.4",
        "symfony/var-dumper": ">=4.4"
    },
    "autoload-dev": {
        "psr-4": {
            "Serafim\\CastXml\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "suggest": {
        "symfony/dom-crawler": "(>=4.4) XML parsing support"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
